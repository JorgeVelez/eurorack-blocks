module Kivu12 {
   board kivu12
   material aluminum
   header { label "Data" }

   data raw {
      file "raw.bin"
   }

   data sample_mono {
      file "mono.wav"
      type AudioSample
   }

   data sample_stereo {
      file "stereo.wav"
      type AudioSample
   }

   control audio_out1 AudioOut {
      position 7.33hp, 111mm
      style thonk.pj398sm.knurled
      label "OUT L"
      pin AO1
   }

   control audio_out2 AudioOut {
      position 10hp, 111mm
      style thonk.pj398sm.knurled
      label "OUT R"
      pin AO2
   }
}
