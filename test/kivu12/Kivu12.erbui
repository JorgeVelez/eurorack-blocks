module Kivu12 {
   board kivu12
   material aluminum
   header { label "Kivu12" }

   control gate_out GateOut {
      position 7.33hp, 111mm
      style thonk.pj398sm.knurled
      label "GATE OUT"
      pin GO2
   }

   control button Button {
      position 7.33hp, 111mm
      style thonk.pj398sm.knurled
      label "BUTTON"
      pin B6
   }

   control led Led {
      position 7.33hp, 111mm
      style thonk.pj398sm.knurled
      label "STATE"
      pin L8
   }

   control freq_pot Pot {
      position 7.33hp, 111mm
      style thonk.pj398sm.knurled
      label "FREQ"
      pin P1
   }

   control freq2_pot Pot {
      position 7.33hp, 111mm
      style thonk.pj398sm.knurled
      label "FREQ"
      pin P2
   }

   control freq_cv CvIn {
      mode bipolar
      position 7.33hp, 111mm
      style thonk.pj398sm.knurled
      label "FREQ"
      pin CI1
   }

   control audio_in2 AudioIn {
      position 10hp, 111mm
      style thonk.pj398sm.knurled
      label "OUT R"
      pin AI2
   }

   control audio_out1 AudioOut {
      position 7.33hp, 111mm
      style thonk.pj398sm.knurled
      label "OUT L"
      pin AO1
   }

   control audio_out2 AudioOut {
      position 10hp, 111mm
      style thonk.pj398sm.knurled
      label "OUT R"
      pin AO2
   }
}
