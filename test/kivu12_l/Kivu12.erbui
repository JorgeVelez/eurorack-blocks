module Kivu12 {
   board kivu12
   material aluminum
   header { label "Kivu12" }

   control pot Pot {
      position 6hp, 40mm
      style rogan
      label "LED"
      pin P1
   }

   control led Led {
      position 6hp, 80mm
      style red
      label "LED"
      pin L1
   }

   control audio_out1 AudioOut {
      position 7.33hp, 111mm
      style knurled
      label "OUT L"
      pin AO1
   }

   control audio_out2 AudioOut {
      position 10hp, 111mm
      style knurled
      label "OUT R"
      pin AO2
   }
}
