module VcvRack {
   width 24hp // 121.92mm
   material aluminum

   header {
      label "VCVRACK TEST"
   }
   footer {
      label "Erb Tests"
   }

   line {
      position 0mm, 0mm
      position 12hp, 10mm
   }
   line {
      position 0mm, 20mm
      position 20mm, 20mm
   }
   line {
      position 10mm, 10mm
      position 10mm, 30mm
   }

   alias audio_in_alias audio_in
   alias audio_out_alias audio_out
   alias button_alias button
   alias led_alias led

   control audio_in AudioInDaisy {
      position 10mm, 20mm
      style thonk.pj398sm.knurled
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin AudioInDaisyPinNC
   }

   control audio_in2 AudioInDaisy {
      position 30mm, 20mm
      style thonk.pj398sm.hex
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin AudioInDaisyPinNC
   }

   control audio_out AudioOutDaisy {
      position 50mm, 20mm
      style thonk.pj398sm.hex
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin AudioOutDaisyPinNC
   }

   control button Button {
      position 70mm, 20mm
      style tl1105
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin PinNC
   }

   control cv_in CvIn {
      position 90mm, 20mm
      style thonk.pj398sm.hex
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin AdcPinNC
   }

   control gate_in GateIn {
      position 110mm, 20mm
      style thonk.pj398sm.hex
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin PinNC
   }

   control gate_out GateOut {
      position 10mm, 40mm
      style thonk.pj398sm.knurled
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin PinNC
   }

   control led Led {
      position 30mm, 40mm
      style led.3mm.red
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin PinNC
   }

   control led_bi LedBi {
      position 50mm, 40mm
      style led.3mm.green_red
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pins PinNC, PinNC
   }

   control pot Pot {
      position 70mm, 40mm
      style rogan.1ps
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin AdcPinNC
   }

   control switch_ Switch {
      position 90mm, 40mm
      style dailywell.2ms1
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pins PinNC, PinNC
   }

   control trim Trim {
      mode bipolar
      position 110mm, 40mm
      style songhuei.9mm
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "B" { positioning bottom }
      pin AdcPinNC
   }

   multiplexer rogan {
      pins AdcPinNC, PinNC, PinNC, PinNC

      control rogan_6ps Pot {
         position 20mm, 65mm
         style rogan.6ps
         label "rogan.6ps" { positioning bottom }
         pin AdcPinNC
      }

      control rogan_5ps Pot {
         position 50mm, 65mm
         style rogan.5ps
         label "rogan.5ps" { positioning bottom }
         pin AdcPinNC
      }

      control rogan_3ps Pot {
         position 73mm, 65mm
         style rogan.3ps
         label "rogan.3ps" { positioning bottom }
         pin AdcPinNC
      }

      control rogan_2ps Pot {
         position 93mm, 65mm
         style rogan.2ps
         label "rogan.2ps" { positioning bottom }
         pin AdcPinNC
      }

      control rogan_1ps Pot {
         position 110mm, 65mm
         style rogan.1ps
         label "rogan.1ps" { positioning bottom }
         pin AdcPinNC
      }
   }

   control dailywell_2ms1 Switch {
      position 10mm, 90mm
      style dailywell.2ms1
      label "dailywell.2ms1" { positioning bottom }
      pins PinNC, PinNC
   }

   control dailywell_2ms3 Switch {
      position 30mm, 90mm
      style dailywell.2ms3
      label "dailywell.2ms3" { positioning bottom }
      pins PinNC, PinNC
   }

   control led_3mm_red Led {
      position 52mm, 85mm
      style led.3mm.red
      label "led.3mm.red" { positioning bottom }
      pin PinNC
   }

   control led_3mm_green Led {
      position 79mm, 85mm
      style led.3mm.green
      label "led.3mm.green" { positioning bottom }
      pin PinNC
   }

   control led_3mm_yellow Led {
      position 52mm, 95mm
      style led.3mm.yellow
      label "led.3mm.yellow" { positioning bottom }
      pin PinNC
   }

   control led_3mm_green_red LedBi {
      position 79mm, 95mm
      style led.3mm.green_red
      label "led.3mm.green_red" { positioning bottom }
      pins PinNC, PinNC
   }

   control led_3mm_rgb LedRgb {
      position 110mm, 100mm
      style led.3mm.rgb
      label "led.3mm.rgb" { positioning bottom }
      pins PinNC, PinNC, PinNC
   }

   control tl1105 Button {
      position 95mm, 85mm
      style tl1105
      label "tl1105" { positioning bottom }
      pin PinNC
   }

   control ck_d6_black Button {
      position 110mm, 85mm
      style ck.d6r.black
      label "ck.d6r.black" { positioning bottom }
      pin PinNC
   }

   control dailywell_2ms1_90ccw Switch {
      position 10mm, 110mm
      style dailywell.2ms1
      rotation 90째ccw
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "90째ccw" { positioning bottom }
      pins PinNC, PinNC
   }

   control dailywell_2ms1_m90cw Switch {
      position 30mm, 110mm
      style dailywell.2ms1
      rotation 90째
      label "L" { positioning left }
      label "T" { positioning top }
      label "R" { positioning right }
      label "90째" { positioning bottom }
      pins PinNC, PinNC
   }

   control thonk_pj398sm_knurled CvIn {
      position 55mm, 110mm
      style thonk.pj398sm.knurled
      label "thonk.pj398sm.knurled" { positioning bottom }
      pin AdcPinNC
   }

   control thonk_pj398sm_hex CvIn {
      position 85mm, 110mm
      style thonk.pj398sm.hex
      label "thonk.pj398sm.hex" { positioning bottom }
      pin AdcPinNC
   }
}
