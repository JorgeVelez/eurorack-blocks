module Field extends daisy_field {

   //--- Oscillator 1 --------------------------------------------------------

   alias osc1_button sw1
   alias osc1_gate gate_in
   alias osc1_state gate_out

   alias osc1_amp pot1
   alias osc1_amp2 cv_in1
   alias osc1_lvl cv_out1

   alias osc1_freq pot2
   alias osc1_freq2 cv_in2

   //--- Oscillator 2 --------------------------------------------------------

   alias osc2_button sw2

   alias osc2_amp pot3
   alias osc2_amp2 cv_in3
   alias osc2_lvl cv_out2

   alias osc2_freq pot4
   alias osc2_freq2 cv_in4
}
